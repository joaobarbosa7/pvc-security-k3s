extends: default
rules:
  braces: disable
  brackets: disable
  colons: disable
  commas: disable
  comments: disable
  comments-indentation: disable
  document-end: disable
  document-start: disable
  empty-lines: disable
  hyphens: disable
  indentation: disable
  line-length: disable
  new-lines: disable
  trailing-spaces: disable
  truthy:
    level: warning
  key-duplicates: enable
  new-line-at-end-of-file: enable

